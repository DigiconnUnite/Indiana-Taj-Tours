{
    "name": "indianataj/tour-website",
    "description": "Indiana Taj Tour - Professional tour booking website",
    "type": "project",
    "require": {
        "php": ">=7.4",
        "phpmailer/phpmailer": "^6.5"
    },
    "autoload": {
        "psr-4": {
            "IndianaTajTour\\": "src/"
